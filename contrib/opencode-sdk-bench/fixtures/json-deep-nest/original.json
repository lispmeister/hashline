{
  "service": {
    "name": "api-gateway",
    "version": "2.1.0",
    "config": {
      "database": {
        "primary": {
          "host": "db.prod.example.com",
          "port": 5432,
          "ssl": true,
          "pool": {
            "min": 2,
            "max": 10
          }
        },
        "replica": {
          "host": "db-replica.prod.example.com",
          "port": 5432,
          "ssl": true,
          "pool": {
            "min": 1,
            "max": 5
          }
        }
      },
      "cache": {
        "enabled": true,
        "ttl": 300
      }
    }
  }
}
